<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>



<VBox prefHeight="300.0" prefWidth="421.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="org.screen_time_tracker.screen_time_tracker.Controller.HelloController">
    <BorderPane prefHeight="300.0" prefWidth="421.0">
        <!-- Other components can go here, in the top, left, right, or center areas if needed -->

        <!-- Button added to the bottom area of the BorderPane -->
        <bottom>
            <Button fx:id="SubmitButton"
                    text="Submit"
                    BorderPane.alignment="CENTER"
                    onAction="#OnSubmitButtonClick"/>
        </bottom>
    </BorderPane>
</VBox>
